{{#partial "content"}}


                <div class="dep">
                    <h2 style="font-weight:bold">Add Department</h2>
                    <form action="/two" method="post">
                        <div class="form-group" >
                            <label for="name">Name</label>
                            <input type="text" id="name" name="name" class="form-control" placeholder="type name ..." required>
                        </div>
                        <div class="form-group">
                            <label for="employees">Employees</label>
                            <input type="number" name="employees" class="form-control" min="1" max="100">
                        </div>
                        <div class="form-group">
                            <label for="description">Description</label>
                            <input  type="text" name="description" class="form-control" placeholder="write something ..." required>
                        </div>
                        <div class="form-group">
                            <button type="submit" style="background-color: #4682B4; color: white;">Add Department</button>
                        </div>
                    </form>
                </div>


{{/partial}}

{{> layout.hbs}}