{{#partial "content"}}



<div class="dep">
    <h2 style="font-weight:bold">Add User</h2>
    <form action="/five" method="post">
        <div class="form-group" >
            <label for="positions">Positions</label>
            <input type="text" id="positions" name="positions" class="form-control" placeholder="type positions ..." required>
        </div>
        <div class="form-group">
            <label for="department_id">Department_ID</label>
            <input type="number" name="department_id" class="form-control" min="1" max="15">
        </div>
        <div class="form-group">
            <label for="role">Role</label>
            <input  type="text" name="role" class="form-control" placeholder="your role ..." required>
        </div>
        <div class="form-group">
            <button type="submit" style="background-color: #4682B4; color: white;">Add User</button>
        </div>
    </form>
</div>



{{/partial}}

{{> layout.hbs}}