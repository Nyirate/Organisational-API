{{#partial "content"}}

                <div class="dep">
                    <h2 style="font-weight:bold">What's New</h2>
                    <form action="/eight" method="post">
                        <div class="form-group" >
                            <label for="heading">Heading</label>
                            <input type="text" id="heading" name="heading" class="form-control" placeholder="heading ..." required>
                        </div>
                        <div class="form-group">
                            <label for="news">News</label>
                            <select name="news" style="color:black">
                                <option value=""></option>
                                <option value="general news">General News</option>
                                <option value="department news">Department News</option>
                            </select>
                            <br>

                        </div>
                        <div class="form-group">
                            <label for="content">Content</label>
                            <input  type="text" name="content" class="form-control" placeholder="What's new ..." required>
                        </div>
                        <div class="form-group">
                            <button type="submit" style="background-color: #4682B4; color: white;">Add News</button>
                        </div>
                    </form>
                </div>
            </div>


{{/partial}}

{{> layout.hbs}}